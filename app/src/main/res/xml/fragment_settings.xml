<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <CheckBoxPreference android:key="settings_saveToCloud"
                        android:title="@string/settings_cloud_title"
                        android:defaultValue="false"
        />
    <ListPreference android:key="settings_timeout"
                    android:title="@string/settings_timeout_title"
                    android:defaultValue="5"
                    android:entryValues="@array/timeoutValues"
                    android:entries="@array/timeoutStrings"
        />
    <Preference android:key="settings_changePassword"
                android:title="@string/settings_password_title" />
    <Preference android:key="settings_importFile"
                android:title="@string/settings_import_title" />
    <Preference android:key="settings_deleteFile"
                android:title="@string/settings_delete_title" />
    <Preference android:key="settings_about"
                android:title="@string/settings_about_title" />

</PreferenceScreen>